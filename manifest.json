  {
    "name": "Umich Leccap Video Downloader",
    "version": "1.0",
    "permissions": ["activeTab", "declarativeContent", "storage","downloads"],
	"background":{
		"scripts":["background.js","popup.js","content.js"],
		"persistent":false	
	},
	"page_action": {
		"default_popup":"popup.html"
	},
   "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }, 
	"description": "Go to your lecture recording, click on the extension and share the link or download the video!",
    "manifest_version": 2
  }
